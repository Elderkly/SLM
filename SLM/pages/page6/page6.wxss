.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  height: calc(100vh - 50rpx);
  background: #fff;
}
.box{
  width: 100%;
  flex: 1;
  padding: 25rpx;
  box-sizing: border-box;
}
.box text{
  font-size: 32rpx;
  font-weight: 500;
}
ec-canvas {
  width: 100%;
  height: 300rpx;
}
.content{
  text-align: center;
  font-size: 26rpx;
  color: #999;
  margin-top: 50rpx;
}