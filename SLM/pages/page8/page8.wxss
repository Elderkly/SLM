/* pages/page8/page8.wxss */
#box{
  padding-bottom: 200rpx;
}
.index-top1{
  display: flex;
  margin: 0rpx auto 10rpx;
  background: #fff;
  width: 95%;
  border-radius: 8rpx;
}
.top1-photo{
  width: 150rpx;
  height: 150rpx;
  margin-top: 30rpx;
  margin-left: 20rpx;
}
.top1-school {
  font-size: 36rpx;
  font-weight: 800;
  color: #000;
  margin-left: 30rpx;
  margin-top: 20rpx;
  height: 30rpx;
}
.top1-fantan{
  font-size: 30rpx;
  font-weight: 400;
  color: #000;
  margin-left: 30rpx;
  margin-top: 20rpx;
}

.top1-text{
  display: flex;
}
.top1-food{
  font-size: 30rpx;
  font-weight: 400;
  color: #000;
  margin-left: 30rpx;
  margin-top: 10rpx;
  flex: 1;
}
.top1-time{
  font-size: 30rpx;
  font-weight: 400;
  color: #000;
  margin-right: 15rpx;
  margin-top: 10rpx;
}
.index-top1>view:nth-child(2){
  flex: 1;
}
.index-botton{
  width: 80%;
  height: 120rpx;
  border-radius: 60rpx;
  background: #4ba6f0;
  /* margin: auto; */
  /* margin-top: 100rpx; */
  font-size: 40rpx;
  color: #fff;
  font-weight: bold;
  text-align: center;
  line-height: 120rpx;
  position: fixed;
  bottom:50rpx;
  left: 80rpx;
}